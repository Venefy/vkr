                    </DVC:Chart>
                </WrapPanel>
                <WrapPanel>
                    <TextBlock Name="MNK1" TextWrapping="Wrap" Text=""/>
                </WrapPanel>
            </StackPanel>
          

        </ScrollViewer>
    </Grid>
</Page>
